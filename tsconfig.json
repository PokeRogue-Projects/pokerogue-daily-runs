{
  "compilerOptions": {
    "target": "esnext",                                 
    "lib": ["dom", "esnext"],                                       
    "jsx": "react",                               
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "preserveSymlinks": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": ["./src/**/*", "./gatsby-node.ts", "./gatsby-config.ts", "./plugins/**/*"]
}
